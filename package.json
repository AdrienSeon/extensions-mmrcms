{
  "name": "extensions-mmrcms",
  "version": "1.0.0",
  "repositoryName": "[PROMISES] mMRCMS Extensions",
  "description": "Contains Paperback sources using the \"my Manga Reader CMS\" template.",
  "scripts": {
    "start": "tsc && node dist/api.js",
    "build": "tsc",
    "test": "node_modules/.bin/mocha --timeout 300000 -r ts-node/register src/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "bundle": "paperback bundle",
    "serve": "paperback serve -p 8000"
  },

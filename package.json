{
  "name": "extensions-mmrcms",
  "version": "1.0.0",
  "description": "Contains Paperback sources using the \"my Manga Reader CMS\" template.",
  "scripts": {
    "start": "tsc && node dist/api.js",
    "build": "tsc",
    "test": "node_modules/.bin/mocha --timeout 300000 -r ts-node/register src/**/*.test.ts --grep 'Testing home page results for CURRENTLY TRENDING titles'",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "bundle": "paperback bundle --sourceslocation=sources",
    "serve": "paperback serve -p 8000 --nobundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdrienSeon/extensions-mmrcms.git"
  },
  "author": "Ankah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AdrienSeon/extensions-mmrcms/issues"
  },
  "homepage": "https://github.com/AdrienSeon/extensions-mmrcms#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.5",
    "paperback-cli": "github:AdrienSeon/paperback-cli",
    "paperback-extensions-common": "^2.1.18",
    "ts-mocha": "^7.0.0",
    "tsify": "^5.0.2",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "typedoc": "^0.17.6"
  }
}

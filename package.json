{
  "name": "extensions-mmrcms",
  "version": "1.0.0",
  "description": "Contains Paperback sources using the \"my Manga Reader CMS\" template.",
  "scripts": {
    "start": "tsc && node dist/api.js",
    "build": "tsc",
    "test": "node_modules/.bin/mocha --timeout 300000 -r ts-node/register src/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "bundle": "paperback bundle",
    "serve": "paperback serve -p 8000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdrienSeon/extensions-mmrcms.git"
  },
  "author": "Ankah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AdrienSeon/extensions-mmrcms/issues"
  },
  "homepage": "https://github.com/AdrienSeon/extensions-mmrcms#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.5",
    "paperback-cli": "^1.2.5",
    "paperback-extensions-common": "^2.1.18",
    "ts-mocha": "^7.0.0",
    "ts-node": "^7.0.1",
    "tsify": "^5.0.2",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "typedoc": "^0.17.6"
  }
}

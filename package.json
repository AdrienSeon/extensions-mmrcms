  },
  "homepage": "https://github.com/AdrienSeon/extensions-mmrcms#readme",
  "dependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cheerio": "^1.0.0-rc.6",
    "paperback-cli": "^1.3.3",
    "paperback-extensions-common": "^3.3.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsify": "^5.0.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.39",
